<layout version="0.1.0">
    <adminhtml_process_list>
        <reference name="head">
            <action method="addCss">
                <name>hackathon_indexerstats.css</name>
            </action>
            <action method="addJs">
                <name>hackathon_indexerstats/index_progress.js</name>
            </action>
        </reference>
        <reference name="process">
            <block type="adminhtml/template" name="process.edit.js" template="hackathon/indexerstats/process/js.phtml"/>
        </reference>
    </adminhtml_process_list>
</layout>